{
    "name": "user-service",
    "version": "1.0.0",
    "description": "Node.js + Express backend with MongoDB, TypeScript, and MVC architecture",
    "main": "dist/server.js",
    "scripts": {
        "start": "node dist/server.js",
        "dev": "nodemon --exec ts-node src/server.ts",
        "build": "tsc",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "format": "prettier --write \"src/**/*.ts\"",
        "format:check": "prettier --check \"src/**/*.ts\"",
        "test": "jest --watchAll=false --no-cache"
    },
    "keywords": [
        "nodejs",
        "express",
        "mongodb",
        "typescript",
        "mvc",
        "authentication"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "amqp-connection-manager": "^5.0.0",
        "amqplib": "^0.10.9",
        "bcryptjs": "^2.4.3",
        "cloudinary": "^2.8.0",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "jsonwebtoken": "^9.0.2",
        "mongoose": "^7.6.3",
        "multer": "^2.0.2",
        "redis": "^5.10.0",
        "socket.io": "^4.8.1",
        "streamifier": "^0.1.1",
        "zod": "^4.2.1"
    },
    "devDependencies": {
        "@types/amqplib": "^0.10.8",
        "@types/bcryptjs": "^2.4.6",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jest": "^30.0.0",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/multer": "^2.0.0",
        "@types/node": "^20.10.0",
        "@types/redis": "^4.0.11",
        "@types/socket.io": "^3.0.2",
        "@types/socket.io-client": "^3.0.0",
        "@types/streamifier": "^0.1.2",
        "@types/supertest": "^6.0.3",
        "@typescript-eslint/eslint-plugin": "^6.13.2",
        "@typescript-eslint/parser": "^6.13.2",
        "eslint": "^8.55.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.0.1",
        "jest": "^30.2.0",
        "mongodb-memory-server": "^11.0.0",
        "nodemon": "^3.0.1",
        "prettier": "^3.1.0",
        "socket.io-client": "^4.8.1",
        "supertest": "^7.1.4",
        "ts-jest": "^29.4.6",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
    }
}
